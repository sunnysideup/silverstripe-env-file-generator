{
  "name": "sunnysideup/env-file-generator",
  "description": "Set some basic variables using a yml file and this module will spit out a nicely formatted .env file",
  "type": "silverstripe-vendormodule",
  "keywords": [
    ".env",
    "dev-tools",
    "security",
    "dev tool",
    "developers"
  ],
  "license": "BSD-3-Clause",
  "require": {
    "silverstripe/framework": "^5.0",
    "silverstripe/admin": "^2.0"
  },
  "require-dev": {
    "sunnysideup/sswebpack_engine_only": "dev-master",
    "silverleague/ideannotator": "*",
    "phpunit/phpunit": "^9.6",
    "squizlabs/php_codesniffer": "^3.7",
    "silverstripe/standards": "^1",
    "phpstan/extension-installer": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "Sunnysideup\\EnvFileGenerator\\": "src/"
    }
  },
  "extra": {
    "expose": [
      "client/dist"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "bin": [
    "bin/npm-build-env-file-generator.sh",
    "bin/npm-watch-env-file-generator.sh"
  ],
  "authors": [
    {
      "name": "Nicolaas Thiemen Francken",
      "email": "modules@sunnysideup.co.nz",
      "role": "lead developer"
    }
  ]
}
