{
    "name": "sunnysideup/env-file-generator",
    "description": "Set some basic variables using a yml file and this module will spit out a nicely formatted .env file",
    "type": "silverstripe-vendormodule",
    "keywords": [
        ".env",
        "dev-tools",
        "security",
        "dev tool",
        "developers"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "mustangostang/spyc": "*"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "Sunnysideup\\EnvFileGenerator\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Nicolaas Thiemen Francken",
            "email": "modules@sunnysideup.co.nz",
            "role": "lead developer"
        }
    ]
